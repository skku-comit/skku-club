  "name": "skku-club",
  "packageManager": "pnpm@8.15.3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:build": "prisma generate",
    "db:push": "prisma db push --accept-data-loss",
    "dev": "pnpm run db:push && next dev",
